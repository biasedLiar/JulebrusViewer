@using BlazorApp1.Models
<div @attributes="AdditionalAttributes">
    <h3 class="PersonSummaryBox_name">@personModel.navn</h3>
    <img class="PersonSummaryBox_img" src="https://thispersondoesnotexist.com/" alt="new person" width="100">
    <h4 class="PersonSummaryBox_events">Events: @personModel.event_count</h4>
    <h4 class="PersonSummaryBox_guesses">Guesses: @personModel.gjett_count</h4>
</div>



@code {
    [Parameter]
    public required ExpandedPersonModel personModel {get; set;}

    [Parameter(CaptureUnmatchedValues = true)]
    public required Dictionary<string, object> AdditionalAttributes { get; set; }
}